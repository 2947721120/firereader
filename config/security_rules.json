{
   "rules": {
      "meta": {
         // list of standard feeds are read only
         ".read": true,
         ".write": false
      },
      "user": {
         "demo": {
            "demo": {
               ".read": true
            }
         },
         "$provider": {
            "$user_id": {
               ".read": "auth.id === $user_id && auth.provider === $provider",
               ".write": "auth.id === $user_id && auth.provider === $provider"
            }
         }
      }
   }
}